{
  "name": "ensure-port",
  "version": "1.5.0",
  "description": "Makes sure you get an available port",
  "type": "module",
  "main": "dist/cjs/index.js",
  "typings": "dist/cjs/index.d.ts",
  "module": "dist/mjs/index.js",
  "scripts": {
    "build": "tsc --build",
    "postbuild": "node scripts/prepare-output-packages-type",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "pretest": "npm run build",
    "test": "mocha \"dist/test/**/*.test.js\" --parallel",
    "watch": "npm run build -- -w"
  },
  "files": [
    "dist/cjs",
    "dist/mjs"
  ],
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "module": "./dist/mjs/index.js",
      "default": "./dist/cjs/index.js",
      "types": "./dist/mjs/index.d.ts"
    }
  },
  "keywords": [
    "typescript",
    "javascript",
    "port",
    "nodejs",
    "http",
    "fs"
  ],
  "author": {
    "email": "tzachbonfil@gmail.com",
    "name": "tzachbon",
    "url": "https://github.com/tzachbon"
  },
  "license": "MIT",
  "devDependencies": {
    "@file-services/memory": "^7.0.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^3.0.0",
    "expect": "^28.1.3",
    "jest-util": "^28.1.3",
    "mocha": "^10.0.0",
    "pleb": "^5.1.0",
    "rimraf": "^3.0.2",
    "ts-expect": "^1.3.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@file-services/node": "^7.0.1",
    "@file-services/types": "^7.0.0",
    "create-listening-server": "^1.0.0"
  }
}
