  "description": "Makes sure you get an available port",
  "main": "dist/cjs/index.js",
  "typings": "dist/mjs/index.d.ts",
  "module": "dist/mjs/index.js",
  "scripts": {
    "build": "tsc --build",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "pretest": "npm run build",
