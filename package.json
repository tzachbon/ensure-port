{
  "name": "ensure-port",
  "version": "1.5.1",
  "description": "Makes sure you get an available port",
  "type": "module",
  "files": [
    "dist/es",
    "dist/cjs"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/es/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/es/index.d.ts",
      "default": "./dist/es/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.cts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "bunchee",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "pretest": "npm run build && tsc -p test/tsconfig.json",
    "test": "mocha \"dist/test/**/*.test.js\" --parallel",
    "watch": "npm run build -- -w"
  },
  "keywords": [
    "typescript",
    "javascript",
    "port",
    "nodejs",
    "http",
    "fs"
  ],
  "author": {
    "email": "tzachbonfil@gmail.com",
    "name": "tzachbon",
    "url": "https://github.com/tzachbon"
  },
  "license": "MIT",
  "devDependencies": {
    "@file-services/memory": "^9.2.1",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "bunchee": "^5.1.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "expect": "^29.7.0",
    "jest-util": "^29.7.0",
    "mocha": "^10.4.0",
    "pleb": "^7.0.0",
    "rimraf": "^5.0.5",
    "ts-expect": "^1.3.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@file-services/node": "^9.2.1",
    "@file-services/types": "^9.2.1",
    "create-listening-server": "^2.1.0"
  }
}