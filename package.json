    "dist/mjs"
  ],
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "keywords": [
    "typescript",
    "javascript",
    "port",
    "nodejs",
    "http",
    "fs"
  ],
